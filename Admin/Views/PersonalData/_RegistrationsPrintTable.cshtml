@model IEnumerable<PersonalDataTable>

<link rel="stylesheet" href="/css/en/bootstrap.min.css">

<table class="table table-striped table-bordered datatable" style="width:100%">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Age)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cities)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Desire)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Genders)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedOnUtc)
            </th>



        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Phone)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Age)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cities)
                </td>


                <td>
                    @Html.DisplayFor(modelItem => item.Desire)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Genders)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreatedOnUtc)
                </td>
            </tr>
        }
    </tbody>
</table>